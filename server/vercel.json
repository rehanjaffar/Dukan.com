{
    "version":2,
    "builds": [
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"dist/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/",
            "dist":"dist/index.html"
        },
        {
            "src":"/(.+)",
            "dist":"dist/&1"
        }
    ]


}
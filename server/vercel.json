{
    "version":2,
    "builds": [
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"dist/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
{
            "src":"/api/auth",
            "dest":"index.js"
        },
        {
            "src":"/",
            "dest":"dist/index.html"
        },
{
            "src":"/login",
            "dest":"dist/index.html"
        },
{
            "src":"/register",
            "dest":"dist/index.html"
        },
        {
            "src":"/(.+)",
            "dest":"dist/$1"
        }
    ]


}